#!/bin/sh
#
# SCRIPT:
#  gacode_getversion
#
# FUNCTION:
#  Get VERSION 
#--------------------------------

file=`which git`

if [ -e "$file" ] && [ -d "$GACODE_ROOT/.git" ] 
then
   cd $GACODE_ROOT
   VERSION=`git log --pretty=format:'%h' -n 1`
else
   # Git not available.  That's bad.
   VERSION="Version not available."
fi

echo $VERSION
